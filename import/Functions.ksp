function fill1DArray(arr, val)
	declare i

	for i := 0 to arr.SIZE_D1-1
		arr[i] := val
	end for
end function

function fill2DArray(arr, val)

	declare i
	declare j

	for i := 0 to arr.SIZE_D1-1
		for j:= 0 to arr.SIZE_D2-1
			arr[i,j] := val
		end for
	end for
end function

function Time_linear(v1, v2, time) -> result
	result := (v2 - v1) / time
end function

function Time_getCurrentTime -> result
	result := $ENGINE_UPTIME
end function